{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "./components/*"
      ],
      "lib/*": [
        "./lib/*"
      ],
      "types/*": [
        "./types/*"
      ],
      "constants/*": [
        "./constants/*"
      ],
      "assets/*": [
        "./assets/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}